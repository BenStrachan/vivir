<section class="content">
  <div class="box">
    <div class="box-header">
      <h3 class="box-title">Assessment types</h3>
    </div>
    <div class="box-body">
      <p id="notice"><%= notice %></p>

      <p>
        <strong>Name:</strong>
        <%= @assessment_type.name %>
      </p>

      <p>
        <strong>Default monthly repeat:</strong>
        <%= @assessment_type.default_monthly_repeat %>
      </p>

      <%= link_to 'Edit', edit_admin_assessment_type_path(@assessment_type) %> |
      <%= link_to 'Back', admin_assessment_types_path %>
    </div>
  </div>
</section>
