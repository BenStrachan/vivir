<section class="content">
  <div class="box">
    <div class="box-header">
      <h3 class="box-title">Job types</h3>
    </div>
    <div class="box-body">
      <p>
        <strong>Name:</strong>
        <%= @job_type.name %>
      </p>

      <p>
        <strong>Default weekly visits:</strong>
        <%= @job_type.default_weekly_visits %>
      </p>

      <%= link_to 'Edit', edit_admin_job_type_path(@job_type) %> |
      <%= link_to 'Back', admin_job_types_path %>
    </div>
  </div>
</section>
